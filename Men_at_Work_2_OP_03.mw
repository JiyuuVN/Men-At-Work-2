  </Line>
  <Line StartPos="8593" Length="5" Skip="false">
    <Translated>Клиф</Translated>
  </Line>
  <Line StartPos="8620" Length="9" Skip="false">
    <Translated>Ох...</Translated>
  </Line>
  <Line StartPos="8643" Length="3" Skip="false">
    <Translated>Айри</Translated>
  </Line>
  <Line StartPos="8659" Length="46" Skip="false">
