  <Line StartPos="8247" Length="5" Skip="false">
    <Translated>Клиф</Translated>
  </Line>
  <Line StartPos="8274" Length="13" Skip="false">
    <Translated>Ох...</Translated>
  </Line>
  <Line StartPos="8292" Length="5" Skip="false">
    <Translated>Клиф</Translated>
  </Line>
  <Line StartPos="8319" Length="13" Skip="false">
