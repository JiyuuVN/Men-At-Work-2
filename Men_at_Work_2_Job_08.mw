  <Line StartPos="3435" Length="5" Skip="false">
    <Translated>Клиф</Translated>
  </Line>
  <Line StartPos="3462" Length="22" Skip="false">
    <Translated>Ой... Ну да. Простите...</Translated>
  </Line>
  <Line StartPos="3498" Length="4" Skip="false">
    <Translated>Мина</Translated>
  </Line>
  <Line StartPos="3515" Length="14" Skip="false">
    <Translated>Боже, Клиф.</Translated>
  </Line>
  <Line StartPos="3544" Length="4" Skip="false">
    <Translated>Люси</Translated>
  </Line>
  <Line StartPos="3561" Length="55" Skip="false">
