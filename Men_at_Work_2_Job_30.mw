  <Line StartPos="4256" Length="5" Skip="false">
    <Translated>Китти</Translated>
  </Line>
  <Line StartPos="4274" Length="18" Skip="false">
    <Translated>Мина, нашлась!</Translated>
  </Line>
  <Line StartPos="4297" Length="5" Skip="false">
    <Translated>Клиф</Translated>
  </Line>
  <Line StartPos="4324" Length="7" Skip="false">
