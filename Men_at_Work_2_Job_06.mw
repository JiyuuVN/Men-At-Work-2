  <Line StartPos="2663" Length="5" Skip="false">
    <Origin>Cliff</Origin>
    <Translated>Клиф</Translated>
  </Line>
  <Line StartPos="2690" Length="36" Skip="false">
    <Origin>"We have to go rescue Hanna-\0san..."</Origin>
