  <Line StartPos="6433" Length="4" Skip="false">
    <Origin>Lucy</Origin>
    <Translated>Люси</Translated>
  </Line>
  <Line StartPos="6450" Length="25" Skip="false">
    <Origin>"Let&apos;s take it slow, ok?"</Origin>
